{
  "manifest_version": 2,

  "name": "Image Helper",
  "description": "List all images on page, ordered by size (dimensions)",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Image Helper"
  },
  "permissions": [
    "activeTab"
  ]
}
